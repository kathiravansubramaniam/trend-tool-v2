# OpenAI
OPENAI_API_KEY=sk-...

# Google Cloud Storage
GCS_BUCKET_NAME=your-trend-pdfs-bucket
GCS_PROJECT_ID=your-gcp-project-id
GCS_CREDENTIALS_PATH=./gcp/service-account-key.json

# Models
PARSE_MODEL=gpt-4o-mini
QUERY_MODEL=gpt-4o

# Parsing
MAX_CONCURRENT_PARSES=5
MAX_PARSE_TOKENS=8000

# Paths
LOCAL_PDF_CACHE=./data/pdf_cache
INDEX_DB_PATH=./data/index.db

# Trigger mode: polling or pubsub
TRIGGER_MODE=polling
POLLING_INTERVAL_SECONDS=60

# Query
MAX_QUERY_DOCS=8
MAX_CONTEXT_TOKENS=14000
